{
  "name": "server_boilerplate",
  "version": "1.0.0",
  "description": "typescript node.js express.js zod boilerplate",
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/services/revise.service.ts --files",
